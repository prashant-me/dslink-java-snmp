{
  "name": "dslink-java-snmp",
  "version": "0.1.15",
  "description": "A dslink for snmp",
  "license": "GNU GPL",
  "main": "bin/dslink-java-snmp",
  "author": {
    "name": "Daniel Shapiro",
    "email": "danielsspam1272@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IOT-DSA/dslink-java-snmp"
  },
  "bugs": {
    "url": "https://github.com/IOT-DSA/dslink-java-snmp/issues"
  },
  "configs": {
  	"name": {
      "type": "string",
      "default": "SNMP"
    },
    "broker": {
      "type": "url"
    },
    "token": {
      "type": "string"
    },
    "nodes": {
      "type": "path",
      "default": "nodes.json"
    },
    "key": {
      "type": "path",
      "default": ".key"
    },
    "log": {
      "type": "string",
      "default": "info"
    },
    "handler_class": {
      "type": "string",
      "default": "snmp.Main"
    }
  }
}
